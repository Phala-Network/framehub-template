{
  "compilerOptions": {
    "target": "es2020",
    "module": "es6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "paths": {
      "@coinbase/onchainkit/dist/lib/*": ["./node_modules/@coinbase/onchainkit/dist/types/*"]
    }
  }
}
